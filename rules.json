{
  "main": [
    {
      "label": "some rule-3",
      "if": [
        { "op": "like", "token": "summary", "value": "(?<first>[a-z]+) (?<second>[a-z]+)" },
        { "op": "eq", "token": "node", "value": "localhost", "uuid": "l" },
        {
          "op": "list",
          "conditions": [
            {
              "op": "list",
              "traversal": "and",
              "conditions": [{ "op": "eq", "token": "group", "value": "production" }]
            }
          ]
        }
      ],
      "then": [
        { "act": "setvar", "varName": "pop", "varValue": "john", "modifier": "append", "uuid": "88" },
        { "act": "template", "varName": "popppy", "template": "from node {{node}}" },
        {
          "act": "rule",
          "uuid": "eee",
          "rule": {
            "label": "subrule",
            "if": [{ "op": "eq", "token": "customer", "value": "GM" }],
            "then": [{ "act": "setvar", "varName": "custy", "varValue": "cars"}],
            "else": []
          }
        }
      ],
      "else": []
    }
  ]
}
